{% extends'template.html.twig' %}

{% block pageTitle %}Liste des totos :{% endblock %}
{% block body %}
<ol class="list-group">
    {% for name, content in app.session.get('todos') %}
        <li class="list-group-item">
            {{ name }} : {{ content }}
        </li>
    {% endfor %}
</ol>

{% endblock %}
{% block title %}
    Todo
{%endblock %}